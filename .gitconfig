[user]
	name = Grenier Celestin
	email = celestin.grenier@hotmail.com
[alias]
	sha = rev-parse HEAD
	url = remote get-url origin
	ls = branch -vav
	cd = checkout
	stat = log --stat
	graph = log --oneline --graph --decorate --simplify-by-decoration
	add-ignore = "!git status --porcelain|sed '/.gitignore/d;s/^?? //p;d'>>.gitignore"
[init]
	defaultBranch = main
[credential]
	helper = store
